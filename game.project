[project]
title = GBJam5
version = 0.1
dependencies = https://github.com/britzl/ludobits/archive/master.zip

[bootstrap]
main_collection = /game/game.collectionc
render = /render/gb.renderc

[input]
game_binding = /input/game.input_bindingc

[display]
width = 640
height = 576

[physics]
scale = 0.02

[script]
shared_state = 1

[graphics]
default_texture_min_filter = nearest
default_texture_mag_filter = nearest

[sprite]
max_count = 1024

